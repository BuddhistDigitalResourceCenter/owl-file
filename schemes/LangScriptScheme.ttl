bdo:LangScript
  a bdo:LangScript ;
  a owl:Class ;
  adm:technicalComment ":LangScript individuals represent combinations of a Language, a script, and possibly a transliteration in which the Language is expressed. For example, bdr:BoEwts represents Tibetan in the Extended Wylie transliteration (which is written in Latin script)"@en ;
  adm:userTooltip "The Language+Script records the combination of language, script, and transliteration in which the content is expresed"@en ;
  rdfs:label "Language+Script"@en ;
  rdfs:seeAlso <http://purl.bdrc.io/ontology/types/LangCode> ;
  rdfs:seeAlso <http://purl.bdrc.io/ontology/types/Language> ;
.
bdo:language
  a owl:ObjectProperty ;
  adm:userTooltip "language of a :LangScript"@en ;
  rdfs:domain bdo:LangScript ;
  rdfs:label "language"@en ;
  rdfs:range bdo:Language ;
.
bdo:script
  a owl:ObjectProperty ;
  rdfs:domain bdo:LangScript ;
  rdfs:label "script"@en ;
  rdfs:range bdo:Script ;
  skos:definition "The script (Tibetan, Devanagari, etc.)"@en ;
.
bdo:transliteration
  a owl:ObjectProperty ;
  adm:translationPriority 2 ;
  rdfs:domain bdo:LangScript ;
  rdfs:label "transliteration"@en ;
  rdfs:range bdo:Transliteration ;
  skos:definition "A transliteration for a LangScript."@en ;
  skos:editorialNote "A transliteration for a :LangScript. Mostly this will apply for Pali situations since texts are always written in a transliteration of Pali in a Romanization or script of a language such as Thai."@en ;
.
