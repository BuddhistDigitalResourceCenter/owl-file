bdr:LicenseAllRightsReserved
  a bdo:License ;
  a skos:Concept ;
  skos:topConceptOf bdr:LicenseScheme ;
  skos:definition "Use it for works under copyright that do not say anything about the copyrights or saying All Rights Reserved."@en ;
  rdfs:seeAlso <http://rightsstatements.org/vocab/InC/1.0/> ;
  skos:prefLabel "All Rights Reserved"@en ;
