  a bdo:Material ;
  a skos:Concept ;
  adm:translationPriority 2 ;
  skos:broader bdr:AppliedMaterial ;
  skos:editorialNote "paper dyed with indigo is called Nīlapattra in some contexts"@en ;
  skos:prefLabel "indigo dye"@en ;
.
bdr:AppliedMaterial_Poison
  a bdo:Material ;
  a skos:Concept ;
  skos:broader bdr:AppliedMaterial ;
  skos:editorialNote "poison (usually arsenic), giving a yellow hue"@en ;
  skos:editorialNote "usually used to keep away insects and people who would want to take part of the manuscript and turn it into a magical tincture for consumption"@en ;
  skos:prefLabel "poison"@en ;
