bdo:Material
  a owl:Class ;
  rdfs:label "Material"@en ;
  rdfs:seeAlso <http://rdaregistry.info/Elements/m/P30304> ;
  rdfs:seeAlso <http://rdaregistry.info/termList/RDAMaterial> ;
  rdfs:subClassOf bf:Material ;
  skos:definition "Materials include the base material on which the printing appears and the applied material such as ink and so on"@en ;
.
bdr:AppliedMaterial
  a bdo:Material ;
  a skos:Concept ;
  adm:translationPriority 2 ;
  rdfs:seeAlso <http://rdaregistry.info/Elements/m/P30084> ;
  skos:definition "The material such as ink and so on that is used to impose writing on the base material"@en ;
  skos:prefLabel "applied material"@en ;
  skos:topConceptOf bdr:MaterialScheme ;
.
bdr:AppliedMaterial_BlackInk
  a bdo:Material ;
  a skos:Concept ;
  skos:broader bdr:AppliedMaterial ;
  skos:prefLabel "black ink"@en ;
.
