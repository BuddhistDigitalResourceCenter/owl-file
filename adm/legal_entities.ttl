adm:LegalData
  a owl:Class ;
  rdfs:comment "A contemporary organization that provides images or eTexts"@en ;
  rdfs:label "Work Access Type"@en ;
.
LegalData:
  a owl:Ontology ;
  owl:imports <http://purl.bdrc.io/ontology/adm/ContentProvider/> ;
  owl:imports <http://purl.bdrc.io/ontology/adm/types/Access/> ;
  owl:imports <http://purl.bdrc.io/ontology/adm/types/License/> ;
  owl:imports <http://purl.bdrc.io/ontology/adm/types/TermsOfUse/> ;
.
bda:LD_BDRC_Open
  a adm:LegalData ;
  adm:access bda:AccessOpen ;
  adm:contentLicense bda:LicensePublicDomain ;
  adm:contentProvider bda:CP_BDRC ;
  adm:copyrightOwner bda:CP_BDRC ;
  adm:license bda:LicensePublicDomain ;
  adm:metadataLicense bda:LicenseCC0 ;
  adm:metadataProvider bda:CP_BDRC ;
  adm:termsOfUse bda:TermsOfUseBDRCGeneral ;
  skos:prefLabel "BDRC open access legal terms"@en ;
.
bda:LD_CUDL
  a adm:LegalData ;
  adm:access bda:AccessOpen ;
  adm:contentProvider bda:CP_CUDL ;
  adm:copyrightOwner bda:CP_CUDL ;
  adm:license bda:LicensePublicDomain ;
  adm:metadataProvider bda:CP_CUDL ;
  adm:termsOfUse <https://cudl.lib.cam.ac.uk/terms/> ;
  skos:prefLabel "CUDL general legal terms"@en ;
.
bda:LD_EAP
  a adm:LegalData ;
  adm:access bda:AccessOpen ;
  adm:contentProvider bda:CP_EAP ;
  adm:copyrightOwner bda:CP_EAP ;
  adm:license bda:LicensePublicDomain ;
  adm:metadataProvider bda:CP_EAP ;
  adm:termsOfUse bda:TermsOfUseBDRCGeneral ;
  skos:prefLabel "EAP general legal terms"@en ;
.
bda:LD_GRETIL
  a adm:LegalData ;
  adm:access bda:AccessOpen ;
  adm:contentProvider bda:CP_GRETIL ;
  adm:copyrightOwner bda:CP_GRETIL ;
  adm:license bda:LicensePublicDomain ;
  adm:metadataProvider bda:CP_GRETIL ;
  adm:termsOfUse bda:TermsOfUseBDRCGeneral ;
  skos:prefLabel "GRETIL general legal terms"@en ;
