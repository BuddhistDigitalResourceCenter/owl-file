# baseURI: http://purl.bdrc.io/ontology/adm/types/Status
# imports: http://purl.bdrc.io/ontology/core/

@prefix Status: <http://purl.bdrc.io/ontology/adm/types/Status/> .
@prefix adm: <http://purl.bdrc.io/ontology/admin/> .
@prefix bdd: <http://purl.bdrc.io/data/> .
@prefix bdo: <http://purl.bdrc.io/ontology/core/> .
@prefix bdr: <http://purl.bdrc.io/resource/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.bdrc.io/ontology/adm/types/Status>
  a owl:Ontology ;
  owl:imports bdo: ;
.
Status:
  a owl:Ontology ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
adm:ImagesStatus
  a owl:Class ;
  rdfs:comment "asset manager status constants for images assets"@en ;
  rdfs:label "Images Status"@en ;
.
adm:Status
  a owl:Class ;
  rdfs:comment "status constants for :Entity metadata: released, provisional, etc"@en ;
  rdfs:label "Status"@en ;
.
adm:imagesStatus
  a owl:AnnotationProperty ;
  rdfs:comment "records the images asset status for a :PublishedWork"@en ;
  rdfs:domain bdo:PublishedWork ;
  rdfs:label "has images status"@en ;
  rdfs:range adm:ImagesStatus ;
.
adm:status
  a owl:AnnotationProperty ;
  rdfs:comment "records the status of the associated resource metadata"@en ;
  rdfs:domain bdo:Entity ;
  rdfs:label "has status"@en ;
  rdfs:range adm:Status ;
.
bdr:StatusAccessioned
  a adm:ImagesStatus ;
  skos:prefLabel "accessioned"@en ;
.
bdr:StatusAcquiring
  a adm:ImagesStatus ;
  skos:prefLabel "acquiring"@en ;
.
bdr:StatusEditing
  a adm:Status ;
  skos:prefLabel "editing"@en ;
.
bdr:StatusOnHold
  a adm:Status ;
  skos:prefLabel "on hold"@en ;
.
bdr:StatusProvisional
  a adm:Status ;
  skos:prefLabel "provisional"@en ;
.
bdr:StatusReleased
  a adm:Status ;
  skos:prefLabel "released"@en ;
.
bdr:StatusScanned
  a adm:ImagesStatus ;
  skos:prefLabel "scanned"@en ;
.
bdr:StatusSeekingOut
  a adm:ImagesStatus ;
  skos:prefLabel "seeking out"@en ;
.
bdr:StatusWithdrawn
  a adm:Status ;
  skos:prefLabel "withdrawn"@en ;
.
