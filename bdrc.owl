    <!-- http://purl.bdrc.io/ontology/core/WorkTitle -->

    <owl:Class rdf:about="http://purl.bdrc.io/ontology/core/WorkTitle">
        <rdfs:subClassOf rdf:resource="http://purl.bdrc.io/ontology/core/Facet"/>
        <rdfs:comment xml:lang="en">A title for the Work including the title text and title type: full, sub, etc:


bdr:W22083 a :Work ;
      :workHasTitle [ a :WorkBibliographicalTitle ;
            rdfs:label &quot;bka&apos; &apos;gyur (stog pho brang bris ma/)&quot;@bo-x-ewts ] ,
