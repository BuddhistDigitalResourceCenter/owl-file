          [ a :WorkTitlePageTitle ;
            rdfs:label &quot;the tog palace manuscript of the tibetan kanjur&quot;@en ] .</rdfs:comment>
        <rdfs:label xml:lang="en">Title</rdfs:label>
    </owl:Class>
    


