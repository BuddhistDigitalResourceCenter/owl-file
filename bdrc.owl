    <!-- http://purl.bdrc.io/ontology/core/ISBN -->

    <rdfs:Datatype rdf:about="http://purl.bdrc.io/ontology/core/ISBN">
        <dataTypeExpression>^(97(8|9))?\d{9}(\d|X)$</dataTypeExpression>
        <rdfs:comment xml:lang="en">The regex is does not take into account opitional blank, &apos; &apos;, or dash, &apos;-&apos;, chars separating the various groups. Also there is no spec for calculating the check digit</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.isbn-international.org/sites/default/files/ISBN%20Manual%202012%20-corr.pdf"/>
        <rdfs:label xml:lang="en">International Standard Book Number</rdfs:label>
    </rdfs:Datatype>
    


    <!-- http://purl.bdrc.io/ontology/core/LCCN -->

    <rdfs:Datatype rdf:about="http://purl.bdrc.io/ontology/core/LCCN">
        <dataTypeExpression>[[a-z]|[[a-z][a-z]|dd]|[a-z][[a-z][a-z]|dd]|[a-z][a-z]dd]dddddddd</dataTypeExpression>
        <rdfs:isDefinedBy rdf:resource="http://www.loc.gov/marc/lccn-namespace.html"/>
        <rdfs:label xml:lang="en">Library of Congress Control Number</rdfs:label>
        <owl:equivalentClass rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </rdfs:Datatype>
    


    <!-- http://purl.bdrc.io/ontology/core/LCCallNumber -->

    <rdfs:Datatype rdf:about="http://purl.bdrc.io/ontology/core/LCCallNumber">
        <dataTypeExpression rdf:resource="https://gist.github.com/kurtraschke/560162"/>
        <rdfs:isDefinedBy rdf:resource="https://www.loc.gov/aba/publications/FreeCSM/freecsm.html"/>
        <rdfs:label xml:lang="en">Library of Congress Call Number</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.loc.gov/catdir/cpso/lcco/"/>
    </rdfs:Datatype>
    


    <!-- http://purl.bdrc.io/ontology/core/NoColophon -->

    <rdfs:Datatype rdf:about="http://purl.bdrc.io/ontology/core/NoColophon">
        <rdfs:comment xml:lang="en">Used to indicated that a Work has no colophon vs a Work for which the colophon has not been recorded in the metadata.</rdfs:comment>
        <rdfs:label xml:lang="en">no colophon</rdfs:label>
    </rdfs:Datatype>
    


    <!-- http://purl.bdrc.io/ontology/core/SliceLoc -->

    <rdfs:Datatype rdf:about="http://purl.bdrc.io/ontology/core/SliceLoc">
        <dataTypeExpression xml:lang="en">[\d]+:[\d]+</dataTypeExpression>
        <rdfs:comment xml:lang="en">The location within an :Etext of a character within an chunk. The location is a subtype of xsd:string constructed by concatenating:

    chunk number + &quot;:&quot; + char number within chunk

Both chunk and char number start at one.</rdfs:comment>
        <rdfs:label>Slice Loc</rdfs:label>
    </rdfs:Datatype>
    


