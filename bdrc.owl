        <rdfs:label xml:lang="en">chunk id</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.bdrc.io/ontology/core/Etext"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.bdrc.io/ontology/core/dateApproximate -->

    <owl:DatatypeProperty rdf:about="http://purl.bdrc.io/ontology/core/dateApproximate">
