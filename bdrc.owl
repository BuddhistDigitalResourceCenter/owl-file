    <owl:Ontology rdf:about="http://purl.bdrc.io/ontology/core/">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">BDRC Ontology</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.1</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.bdrc.io/ontology/admin/access -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/access">
        <rdfs:comment xml:lang="en">The type of online access permitted for the subject Work.</rdfs:comment>
        <rdfs:label xml:lang="en">permitted access</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.bdrc.io/ontology/core/AccessType"/>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/core/AccessType"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/LicensedAccessedEntity"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/conversionAnnotations -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/conversionAnnotations"/>
    


    <!-- http://purl.bdrc.io/ontology/admin/convertProperty -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/convertProperty">
        <rdfs:comment xml:lang="en">this annotation indicates that the annotated property is to be converted to some other expression, typically to a form that is expressed in terms of a common 3rd-party ontology such as bibframe or foaf.

The value of this property should be an explanation of the needed conversion as an rdf:langString</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.bdrc.io/ontology/admin/conversionAnnotations"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/convertToClass -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/convertToClass">
        <rdfs:comment xml:lang="en">this annotation is used to indicate that uses of the annotated class are to be replaced by the inidcated class</rdfs:comment>
        <rdfs:label xml:lang="en">convert to Class</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://purl.bdrc.io/ontology/admin/conversionAnnotations"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/convertToProperty -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/convertToProperty">
        <rdfs:comment xml:lang="en">Indicates that the annotated property is to be converted directly to a statement in terms of another property. The value of this annotation is the target property to convert to.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.bdrc.io/ontology/admin/conversionAnnotations"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/convertValueToClass -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/convertValueToClass">
        <rdfs:comment xml:lang="en">Indicates that the value of the property being converted is to be an instance of the designated class</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.bdrc.io/ontology/admin/conversionAnnotations"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/legacyImageGroupRID -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/legacyImageGroupRID">
        <rdfs:comment xml:lang="en">records the original imagegroup rid from which the volume information was migrated</rdfs:comment>
        <rdfs:label xml:lang="en">legacy imagegroup rid</rdfs:label>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/license -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/license">
        <rdfs:comment xml:lang="en">The license under which access to the Work is provided</rdfs:comment>
        <rdfs:label xml:lang="en">has license</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.bdrc.io/ontology/core/License"/>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/core/License"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/LicensedAccessedEntity"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/logDate -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/logDate">
        <rdfs:comment xml:lang="en">When the update was performed</rdfs:comment>
        <rdfs:label xml:lang="en">date</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/admin/LogEntry"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/logEntry -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/logEntry">
        <rdfs:comment xml:lang="en">Log entry for updates to BDRC resources</rdfs:comment>
        <rdfs:label xml:lang="en">log entry</rdfs:label>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/admin/LogEntry"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Entity"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/logMessage -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/logMessage">
        <rdfs:comment xml:lang="en">Explanation of the update</rdfs:comment>
        <rdfs:label xml:lang="en">message</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/admin/LogEntry"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/logWho -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/logWho">
        <rdfs:comment xml:lang="en">who performed the updated being logged</rdfs:comment>
        <rdfs:label xml:lang="en">who</rdfs:label>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/admin/LogEntry"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/outline -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/outline">
        <rdfs:comment xml:lang="en">The referenced admin:Outline contains the root-level information from an Outline for this Work</rdfs:comment>
        <rdfs:label xml:lang="en">had outline</rdfs:label>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/admin/Outline"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Work"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/outlineAuthorStatement -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/outlineAuthorStatement">
        <rdfs:comment xml:lang="en">A statement of the authorship of the outline of this Work</rdfs:comment>
        <rdfs:comment xml:lang="en">Migration hint: use this for &lt;outline:creator/&gt;</rdfs:comment>
        <rdfs:label xml:lang="en">outline author</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/admin/Outline"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/outlineLogEntry -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/outlineLogEntry">
        <rdfs:comment xml:lang="en">Migration hint: Used to migrate the log entries from Outlines that are being converted to many individual works</rdfs:comment>
        <rdfs:label xml:lang="en">outline log entry</rdfs:label>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/admin/LogEntry"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/admin/Outline"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/outlineType -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/outlineType">
        <rdfs:comment xml:lang="en">records the legacy outline type</rdfs:comment>
        <rdfs:label xml:lang="en">outline type</rdfs:label>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/admin/OutlineType"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/admin/Outline"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/place_TLM_accession -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/place_TLM_accession">
        <rdfs:comment>The accession attribute records the next accession number to be allocated for the Library. The num should be a 6 digit zero filled string initialized to &quot;000001&quot;. This number is incremented each time a new holdng is added for this library.</rdfs:comment>
        <rdfs:label xml:lang="en">accession</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Place"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/place_TLM_admin -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/place_TLM_admin">
        <rdfs:comment>The admin property records the name of the group of users that are permitted to update the Library and Monastery profiles for the Library. The group name will conventionally be named by lower-casing the hasTLM_code property value and appending &quot;Admin&quot;. For example, &quot;dzlAdmin&quot;</rdfs:comment>
        <rdfs:label xml:lang="en">tlm admin group</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Place"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/place_TLM_adminEmail -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/place_TLM_adminEmail">
        <rdfs:comment>The admin email is used to send requests for new users to be added to the Library.</rdfs:comment>
        <rdfs:label xml:lang="en">tlm admin email</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Place"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/place_TLM_code -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/place_TLM_code">
        <rdfs:comment>the code attribute records a unique 3-latin-letter code for the Library. For example, DZL would be reasonable for Dzongsar Library. The code along with the num attribute are used to create RIDs for Works that are added by the library. RIDs will take the form of: W1TLMDZL000001</rdfs:comment>
        <rdfs:label xml:lang="en">tlm code</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Place"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/place_TLM_librarian -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/place_TLM_librarian">
        <rdfs:comment xml:lang="en">The librarian property records the name of the group of users that are permitted to add/update/delete Library holdings. The group name will conventionally be named by lower-casing the TLM/code and appending &quot;librarian&quot;. for example, &quot;dzllibrarian&quot;</rdfs:comment>
        <rdfs:label xml:lang="en">tlm librarian group</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Place"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/place_TLM_librarianEmail -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/place_TLM_librarianEmail">
        <rdfs:label xml:lang="en">The librarian email is used to send requests to the library staff.</rdfs:label>
        <rdfs:label xml:lang="en">tlm librarian email</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Place"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/place_TLM_num -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/place_TLM_num">
        <rdfs:comment>The num attribute records the last RID suffix allocated for the Library. The num should be a 6 digit zero filled string initialized to &quot;000001&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">tlm num</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Place"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/place_TLM_taxonomy -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/place_TLM_taxonomy">
        <rdfs:comment xml:lang="en">Identifies a Taxonomy that specifies the Library&apos;s shelf classification. By convention the taxonomy should have maximum depth of 4 levels - this constraint allows for a manageable GUI design.</rdfs:comment>
        <rdfs:label xml:lang="en">TLM taxonomy</rdfs:label>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/core/Taxonomy"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Place"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/productAllowByAddr -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/productAllowByAddr">
        <rdfs:comment xml:lang="en">The prodcutAllowByAddr has an xsd:string with the format of a cidr IP address range, e.g., 128.2.8.0/24. These address ranges are used to permit access to collections of works</rdfs:comment>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/admin/ProductOrg"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/productHasWork -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/productHasWork">
        <rdfs:comment xml:lang="en">the admin:Product contains the referenced :Work</rdfs:comment>
        <rdfs:label xml:lang="en">has work</rdfs:label>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/productIncludes -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/productIncludes">
        <rdfs:comment xml:lang="en">A Product may include another Product for the purpose of including access information in support of the subscription service.</rdfs:comment>
        <rdfs:label xml:lang="en">includes</rdfs:label>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/admin/Product"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/admin/Product"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/status -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/status">
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/core/StatusType"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Entity"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/volumeQcDate -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/volumeQcDate">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Volume"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/volumeQcNote -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/volumeQcNote">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Volume"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/volumeQcPerson -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/volumeQcPerson">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Volume"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/volumeScanVenue -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/volumeScanVenue">
        <rdfs:comment xml:lang="en">this property is used to migrate the venue attribute from scanrequest. it records where the scanning was to take place</rdfs:comment>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Item"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/workInProduct -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/workInProduct">
        <rdfs:comment xml:lang="en">This :Work is included in the referenced admin:Product</rdfs:comment>
        <rdfs:label xml:lang="en">in product</rdfs:label>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/admin/Product"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Work"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/workVolumesNote -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/workVolumesNote">
        <rdfs:comment xml:lang="en">Migration hint: When not empty the &lt;pubinfo:volumes/&gt; is a note regarding how many and which volumes have been acquired, scanned and so on.</rdfs:comment>
        <rdfs:label xml:lang="en">volumes note</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Work"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/work_desc_date -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/work_desc_date">
        <rdfs:comment xml:lang="en">Migration hint: Used only in O01CT0001 to record some sort of date information from the colophons of various texts. The format is non-standard will require further examination</rdfs:comment>
        <rdfs:label xml:lang="en">date from description</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Work"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/core/dataTypeExpression -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/core/dataTypeExpression"/>
    


    <!-- http://purl.bdrc.io/ontology/core/inferSubTree -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/core/inferSubTree">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/core/migration_exception -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/core/migration_exception">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/core/note -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/core/note">
        <rdfs:comment>the :note annotation adds langauge tagged string and optionally a reference to a Work and a simpe text description of a location in a Work, e.g., &quot;pp. 231-3&quot;.

In the XML db the notes are restricted to a limited set of items such as top-level resources and Outline Nodes, Place Events and so on. In this model there are no limitations on where notes can be attached. They can be attached directly to any owl:NamedIndividual or property. In particular one could add a note to the per:name property on an individual that provides evidence for the particular per:name assertion.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/core/Note"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/core/noteLocationStatement -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/core/noteLocationStatement">
        <rdfs:comment xml:lang="en">A location statement for where in the :noteWork to look for the :noteText</rdfs:comment>
        <rdfs:comment xml:lang="en">Migration hint: the @location of the &lt;x:note/&gt;.</rdfs:comment>
        <rdfs:label xml:lang="en">location</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Note"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/core/noteText -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/core/noteText">
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Note"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/core/noteWork -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/core/noteWork">
        <rdfs:comment xml:lang="en">The source Work for the noteContent</rdfs:comment>
        <rdfs:label xml:lang="en">work</rdfs:label>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/core/Work"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Note"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/core/seeAlso -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/core/seeAlso">
        <rdfs:comment xml:lang="en">Indicates that another Topic is related in someway to this Topic. Eventually this will need to be clarified in terms of prior practice</rdfs:comment>
        <rdfs:label xml:lang="en">see other topic</rdfs:label>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/core/Topic"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Topic"/>
