        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/Item"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">The number of volumes in the item</rdfs:comment>
