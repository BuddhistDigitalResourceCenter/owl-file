    <!-- http://purl.bdrc.io/ontology/core/attributionURL -->

    <owl:DatatypeProperty rdf:about="http://purl.bdrc.io/ontology/core/attributionURL">
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/TextualEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:label xml:lang="en">attribution url</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://creativecommons.org/ns#attributionURL"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.bdrc.io/ontology/core/chunkContents -->

    <owl:DatatypeProperty rdf:about="http://purl.bdrc.io/ontology/core/chunkContents">
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/EtextChunk"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
        <rdfs:label xml:lang="en">contents</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.bdrc.io/ontology/core/Etext"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.bdrc.io/ontology/core/chunkId -->

    <owl:DatatypeProperty rdf:about="http://purl.bdrc.io/ontology/core/chunkId">
