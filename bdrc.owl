            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The value is the legacy Outline Node RID so that redirection can be implemented for purls referencing nodes on the old www.tbrc.org site.</rdfs:comment>
        <rdfs:label xml:lang="en">has legacy node</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.bdrc.io/ontology/core/DDBCAId -->

    <owl:DatatypeProperty rdf:about="http://purl.bdrc.io/ontology/core/DDBCAId">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.bdrc.io/ontology/core/Person"/>
                    <rdf:Description rdf:about="http://purl.bdrc.io/ontology/core/Place"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="en">Buddhist Studies Authority ID</rdfs:comment>
        <rdfs:comment xml:lang="en">ID used by the Dharma Drum College Buddhist Studied Authorities databases, for places and persons. It should be just the ID starting with A (ex: A017001), not the full URL.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://authority.dila.edu.tw/person/"/>
        <rdfs:seeAlso rdf:resource="http://authority.dila.edu.tw/place/"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.bdrc.io/ontology/core/attributionName -->

    <owl:DatatypeProperty rdf:about="http://purl.bdrc.io/ontology/core/attributionName">
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/core/TextualEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
        <rdfs:comment xml:lang="en">The name the holder of a Work resource would like used when attributing re-use.</rdfs:comment>
        <rdfs:label xml:lang="en">attribution name</rdfs:label>
