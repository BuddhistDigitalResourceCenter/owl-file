<!-- Scheme: RDA Layout -->
    <skos:ConceptScheme rdf:about="http://rdaregistry.info/termList/layout">
        <dc:title>RDA Layout</dc:title>
    </skos:ConceptScheme>

<!-- Concept: back to back  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1003" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8436"/>
      <skos:prefLabel xml:lang="en">back to back</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">back to back</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout in which the same image is represented in more than one language on each side of a sheet.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout in which the same image is represented in more than one language on each side of a sheet.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to cartographic images.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">back-to-back</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">back-to-back</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: bar by bar  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1004" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8437"/>
      <skos:prefLabel xml:lang="en">bar by bar</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">bar by bar</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of music displayed in indented paragraphs with all of the parts for each measure written consecutively.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of music displayed in indented paragraphs with all of the parts for each measure written consecutively.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">battuta per battuta</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">battuta per battuta</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: bar over bar  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1005" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8438"/>
      <skos:prefLabel xml:lang="en">bar over bar</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">bar over bar</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of measures displayed in units called parallels and that the beginning of each part’s measure is vertically aligned with the beginning of the same measure for the other part or parts.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of measures displayed in units called parallels and that the beginning of each part’s measure is vertically aligned with the beginning of the same measure for the other part or parts.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">battuta sopra battuta</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">battuta sopra battuta</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: both sides  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1006" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8439"/>
      <skos:prefLabel xml:lang="en">both sides</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">both sides</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout that is continued at the same scale on both sides of a sheet or sheets.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout that is continued at the same scale on both sides of a sheet or sheets.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to cartographic images.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">entrambi i lati</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">entrambi i lati</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: double line spacing  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1017" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8450"/>
      <skos:prefLabel xml:lang="en">double line spacing</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">double line spacing</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout containing a full blank line between lines of text.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout containing a full blank line between lines of text.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile text.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">interlinea doppia</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">interlinea doppia</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: double sided  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1001" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/836"/>
      <skos:prefLabel xml:lang="en">double sided</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">double sided</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout that uses both sides of a sheet.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout that uses both sides of a sheet.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to sheets and tactile text.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">fronte retro</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">fronte retro</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: line by line  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1007" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8440"/>
      <skos:prefLabel xml:lang="en">line by line</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">line by line</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of lines of text that alternate with lines of music for the preceding text.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of lines of text that alternate with lines of music for the preceding text.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">linea per linea</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">linea per linea</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: line over line  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1008" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8441"/>
      <skos:prefLabel xml:lang="en">line over line</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">line over line</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of measures displayed in parallels, but the measures are not necessarily aligned.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of measures displayed in parallels, but the measures are not necessarily aligned.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">linea sopra linea</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">linea sopra linea</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: melody chord system  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1009" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8442"/>
      <skos:prefLabel xml:lang="en">melody chord system</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">melody chord system</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of chord symbols and melody presented in paragraphs with music braille used for the chord symbols.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of chord symbols and melody presented in paragraphs with music braille used for the chord symbols.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">sistema di melodia accordale</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">sistema di melodia accordale</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: open score  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1010" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8443"/>
      <skos:prefLabel xml:lang="en">open score</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">open score</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of music presented in parallels.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of music presented in parallels.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">Open score is similar to bar over bar, but used for ensemble scores. This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">partitura a parti late</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">partitura a parti late</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: outline  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1011" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8444"/>
      <skos:prefLabel xml:lang="en">outline</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">outline</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of music in the outline format.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of music in the outline format.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">outline</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">outline</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: paragraph  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1012" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8445"/>
      <skos:prefLabel xml:lang="en">paragraph</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">paragraph</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout in braille consisting of music displayed in paragraphs with one part per paragraph.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout in braille consisting of music displayed in paragraphs with one part per paragraph.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">paragrafo</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">paragrafo</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: section by section  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1013" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8446"/>
      <skos:prefLabel xml:lang="en">section by section</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">section by section</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of hand or part signs that are prominent in the left margin.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of hand or part signs that are prominent in the left margin.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">sezione per sezione</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">sezione per sezione</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: short form scoring  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1014" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8447"/>
      <skos:prefLabel xml:lang="en">short form scoring</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">short form scoring</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of words, chord symbols, and melody presented in parallels with literary braille used for the chord symbols.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of words, chord symbols, and melody presented in parallels with literary braille used for the chord symbols.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">Short form scoring is used chiefly for popular and folk music. This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">partitura a parti strette</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">partitura a parti strette</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: single line  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1015" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8448"/>
      <skos:prefLabel xml:lang="en">single line</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">single line</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of music for single instrumental parts or solos presented in segments of two to five lines, the initial line displaying a measure number or rehearsal designation.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of music for single instrumental parts or solos presented in segments of two to five lines, the initial line displaying a measure number or rehearsal designation.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">linea singola</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">linea singola</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: single sided  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1002" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/837"/>
      <skos:prefLabel xml:lang="en">single sided</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">single sided</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout that uses only one side of a sheet.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout that uses only one side of a sheet.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to sheets and tactile text.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">facciata singola</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">facciata singola</rdakit:toolkitLabel>
    </skos:Concept>
<!-- Concept: vertical score  -->
    <skos:Concept rdf:about="http://rdaregistry.info/termList/layout/1016" xml:lang="en">
      <skos:inScheme rdf:resource="http://rdaregistry.info/termList/layout"/>
      <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001"/>
      <reg:identifier rdf:resource="http://metadataregistry.org/concepts/8449"/>
      <skos:prefLabel xml:lang="en">vertical score</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="en">vertical score</rdakit:toolkitLabel>
      <skos:definition xml:lang="en">A layout consisting of musical parts that are combined and written as chords.</skos:definition>
      <rdakit:toolkitDefinition xml:lang="en">A layout consisting of musical parts that are combined and written as chords.</rdakit:toolkitDefinition>
      <skos:scopeNote xml:lang="en">This term applies to tactile music resources.</skos:scopeNote>
      <skos:prefLabel xml:lang="it">partitura verticale</skos:prefLabel>
      <rdakit:toolkitLabel xml:lang="it">partitura verticale</rdakit:toolkitLabel>
    </skos:Concept>

