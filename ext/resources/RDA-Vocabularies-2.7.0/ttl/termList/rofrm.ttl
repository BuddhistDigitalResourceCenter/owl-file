    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a skos:Concept ;
    skos:definition "Revision through the release of notices of errors, corrections, addenda, etc."@en ;
    skos:inScheme <http://rdaregistry.info/termList/rofrm> ;
    skos:prefLabel "correction"@en .

<http://rdaregistry.info/termList/rofrm/T1002>
