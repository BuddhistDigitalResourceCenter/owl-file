    reg:status <http://metadataregistry.org/uri/RegStatus/1001> ;
    a skos:Concept ;
    skos:definition "The content is not expected to be revised after initial release."@en ;
    skos:inScheme <http://rdaregistry.info/termList/rofrm> ;
    skos:prefLabel "not applicable"@en .

