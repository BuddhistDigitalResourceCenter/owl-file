  adm:userTooltip "This is the location of the source information for this :Lineage in a specified gsan / thob yig."@en ;
  rdfs:domain [
      a owl:Class ;
      owl:unionOf (
          bdo:Lineage
          bdo:Note
          bdo:Instance
        ) ;
    ] ;
  rdfs:label "location"@en ;
  rdfs:range bdo:ContentLocation ;
  rdfs:seeAlso bdo:Lineage ;
.
bdo:contentLocationByFolio
  a owl:DatatypeProperty ;
  adm:technicalComment "was :workLocationByFolio"@en ;
