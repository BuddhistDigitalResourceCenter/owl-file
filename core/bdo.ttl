  rdfs:label "content provider"@en ;
  owl:deprecated true ;
.
bdo:corporationHasMember
  a owl:ObjectProperty ;
  rdfs:domain bdo:Corporation ;
  rdfs:label "has member"@en ;
  rdfs:range bdo:CorporationMember ;
.
bdo:corporationMember
  a owl:ObjectProperty ;
  rdfs:domain bdo:CorporationMember ;
  rdfs:label "person"@en ;
