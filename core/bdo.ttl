bdo:eventPeriod
  a owl:ObjectProperty ;
  rdfs:comment "associates an event with a time period"@en ;
  rdfs:domain bdo:Event ;
  rdfs:label "period"@en ;
  rdfs:range bdo:TimePeriod ;
