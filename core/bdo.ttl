  rdfs:label "image number"@en ;
  rdfs:range bdo:IntegerOrString ;
.
bdo:contentLocationPhrase
  a owl:DatatypeProperty ;
  adm:catalogingConvention "This should be a positive integer counting from the left side of the page tshegs and shads to the phrase (syllable) that is to be indicated by the location. This is optional and only makes sense if there is a :workLocationLine specified as well."@en ;
  adm:technicalComment "was :workLocationPhrase"@en ;
