  rdfs:comment "The name the holder of a Work resource would like used when attributing re-use."@en ;
  rdfs:domain bdo:TextualEntity ;
  rdfs:label "attribution name"@en ;
  rdfs:range rdf:langString ;
  rdfs:seeAlso cc:attributionName ;
.
bdo:attributionURL
