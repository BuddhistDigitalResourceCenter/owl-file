  adm:translationPriority 2 ;
  adm:userTooltip "sequence number of the work location in a list of locations"@en ;
  rdfs:domain bdo:ContentLocation ;
  rdfs:label "location sequence number"@en ;
  rdfs:range xsd:string ;
.
bdo:contentLocationSide
  a owl:DatatypeProperty ;
  adm:catalogingConvention "If :contentLocationByFolio true then side should be either \"recto\" or \"verso\" but will likely need to be normalized since there has not been validation in the editor of this field. Most locations are specified in terms of pages."@en ;
  adm:technicalComment "was :workLocationSide"@en ;
  adm:translationPriority 2 ;
  adm:userTooltip "Starting side of the folio in a folio-based location."@en ;
  rdfs:domain bdo:ContentLocation ;
  rdfs:label "side"@en ;
  rdfs:range xsd:string ;
.
bdo:contentLocationStatement
  a owl:DatatypeProperty ;
