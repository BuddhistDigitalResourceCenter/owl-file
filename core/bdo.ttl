  rdfs:subClassOf bdo:WorkTitle ;
.
bdo:WorkCopyrightPageTitle
  a owl:Class ;
  rdfs:comment "use for the title on the copyright page when it differs from the bibliographical title."@en ;
  rdfs:label "Copyright page Title"@en ;
  rdfs:seeAlso "http://cip.loc.gov/cipman/"^^xsd:anyURI ;
  rdfs:seeAlso "https://www.loc.gov/publish/cip/news/lccn_art.html"^^xsd:anyURI ;
  rdfs:subClassOf bdo:WorkTitle ;
