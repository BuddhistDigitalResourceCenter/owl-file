# baseURI: http://purl.bdrc.io/ontology/types/PartType/
# imports: http://www.w3.org/2004/02/skos/core
# prefix: WorkPartType

@prefix WorkPartType: <http://purl.bdrc.io/ontology/types/PartType/> .
@prefix adm: <http://purl.bdrc.io/ontology/admin/> .
@prefix bdo: <http://purl.bdrc.io/ontology/core/> .
@prefix bdr: <http://purl.bdrc.io/resource/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

bdo:PartType
  a owl:Class ;
  adm:userTooltip "represents the type of a part of an instance, work or item such as Chapter, Section and so on"@en ;
  rdfs:label "Part Type"@en ;
.
PartType:
  a owl:Ontology ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.
bdr:PartTypeChapter
  a bdo:WorkPartType ;
  skos:prefLabel "chapter"@en ;
.
bdr:PartTypeFragment
  a bdo:WorkPartType ;
  rdfs:comment "A fragment is a portion of a \"whole\" instance. It commonly occurs when some pages have been extracted from a \"complete\" instance and inserted improperly into a different one. A fragment is specified as a part of a containing instance via a :ContentLocation as with any similar :partOf"@en ;
  skos:prefLabel "fragment"@en ;
.
bdr:PartTypeSection
  a bdo:WorkPartType ;
  skos:prefLabel "section"@en ;
.
bdr:PartTypeText
  a bdo:WorkPartType ;
  skos:prefLabel "text"@en ;
.
bdr:PartTypeTableOfContent
  a bdo:WorkPartType ;
  skos:prefLabel "table of content"@en ;
  skos:prefLabel "dkar chag"@bo-x-ewts ;
.
bdr:PartTypeVolume
  a bdo:WorkPartType ;
  skos:prefLabel "volume"@en ;
.
