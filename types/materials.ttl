bdo:AppliedMaterial
  a owl:Class ;
  rdfs:label "applied material"@en ;
  rdfs:seeAlso "http://www.rdaregistry.info/Elements/m/#P30084"^^xsd:anyURI ;
  rdfs:subClassOf bdo:Material ;
.
bdo:BaseMaterial
  a owl:Class ;
  rdfs:comment "underlying physical material of a manifestation"@en ;
  rdfs:label "Base material" ;
  rdfs:seeAlso "http://www.rdaregistry.info/Elements/m/#P30208"^^xsd:anyURI ;
  rdfs:subClassOf bdo:Material ;
.
bdo:Material
  a owl:Class ;
  rdfs:comment "This describes the material on which the content of a :TextualEntity is imposed. A physical or chemical substance used to create a manifestation"@en ;
  rdfs:label "material"@en ;
  rdfs:seeAlso "http://www.rdaregistry.info/Elements/m/#P30304"^^xsd:anyURI ;
  rdfs:seeAlso "https://www.rdaregistry.info/termList/RDAMaterial/"^^xsd:anyURI ;
.
Material:
  a owl:Ontology ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
bdr:AppliedMaterial_BlackInk
  a bdo:AppliedMaterial ;
  skos:prefLabel "black ink"@en ;
.
bdr:AppliedMaterial_GoldLeaf
  a bdo:AppliedMaterial ;
  skos:prefLabel "gold leaf"@en ;
.
bdr:AppliedMaterial_RedInk
  a bdo:AppliedMaterial ;
  skos:prefLabel "red ink"@en ;
.
bdr:AppliedMaterial_SilverLeaf
  a bdo:AppliedMaterial ;
