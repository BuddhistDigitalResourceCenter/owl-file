  rdfs:seeAlso <https://www.jstor.org/stable/23657810?seq=1#page_scan_tab_contents> ;
  skos:altLabel "daphne paper"@en ;
  skos:altLabel "dezo paper"@en ;
  skos:prefLabel "deshog paper"@en ;
.
bdr:MaterialDigital
