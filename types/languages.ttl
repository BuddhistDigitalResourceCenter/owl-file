bdo:Language
  a owl:Class ;
  rdfs:comment "A natural Language such as Tibetan, Chinese, Sanskrit, etc."@en ;
  rdfs:label "Language"@en ;
  rdfs:seeAlso <http://id.loc.gov/vocabulary/languages.html> ;
  rdfs:seeAlso "LangCode"@en ;
.
<http://purl.bdrc.io/ontology/types/Language>
  a owl:Ontology ;
  owl:versionInfo "Created with TopBraid Composer" ;
  owl:imports <http://purl.bdrc.io/ontology/types/Taxonomy> ;
.
bdr:LanuageTaxonomy
  a bdo:Language ;
  a bdo:Taxonomy ;
  bdo:taxSubclassRelation bdo:taxSubClassOf ;
  rdfs:comment "Taxonomy of natural languages"@en ;
  skos:prefLabel "languages taxonomy"@en ;
.
bdr:Bo
  a bdo:Language ;
  a bdo:Taxonomy ;
  bdo:taxSubClassOf bdr:LanuageTaxonomy ;
  skos:prefLabel "Tibetan"@en ;
.
bdr:Mn
