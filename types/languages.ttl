  rdfs:range xsd:string ;
  rdfs:seeAlso <http://id.loc.gov/vocabulary/languages.html> ;
  rdfs:seeAlso <http://lexvo.org/ontology#marcCode> ;
  rdfs:seeAlso <https://www.loc.gov/marc/languages/> ;
.
