  skos:prefLabel "Mongolian"@en ;
.
bdr:Pi
  a bdo:Language ;
  a bdo:Taxonomy ;
  bdo:taxSubClassOf bdr:LanuageTaxonomy ;
  skos:prefLabel "Pali"@en ;
.
bdr:Sa
  a bdo:Language ;
  a bdo:Taxonomy ;
  bdo:taxSubClassOf bdr:LanuageTaxonomy ;
  skos:prefLabel "Sanskrit"@en ;
