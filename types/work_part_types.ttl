# baseURI: http://purl.bdrc.io/ontology/types/WorkPartType/
# imports: http://www.w3.org/2004/02/skos/core
# prefix: WorkPartType

@prefix WorkPartType: <http://purl.bdrc.io/ontology/types/WorkPartType/> .
@prefix adm: <http://purl.bdrc.io/ontology/admin/> .
@prefix bdo: <http://purl.bdrc.io/ontology/core/> .
@prefix bdr: <http://purl.bdrc.io/resource/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

bdo:WorkPartType
  a owl:Class ;
  rdfs:comment "represents the type of a part of a Work such as Chapter, Section and so on"@en ;
  rdfs:label "Work Part Type"@en ;
.
WorkPartType:
  a owl:Ontology ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.
bdr:WorkChapter
  a bdo:WorkPartType ;
  skos:prefLabel "chapter"@en ;
.
bdr:WorkFragment
  a bdo:WorkPartType ;
  rdfs:comment "A :WorkFragment is a portion of a \"whole\" work. A :WorkFragment commonly occurs when some pages have been extracted from a \"complete\" :Work and inserted improperly into a different :Work which has then been copied. A fragment is specified as a part of a containing :Work via a :WorkLocation as with any similar :workPartOf"@en ;
  skos:prefLabel "fragment"@en ;
.
bdr:WorkSection
  a bdo:WorkPartType ;
  skos:prefLabel "section"@en ;
.
bdr:WorkText
  a bdo:WorkPartType ;
  skos:prefLabel "text"@en ;
.
bdr:WorkVolume
  a bdo:WorkPartType ;
  skos:prefLabel "volume"@en ;
.
