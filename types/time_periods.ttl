# baseURI: http://purl.bdrc.io/ontology/types/TimePeriod/
# imports: http://www.w3.org/2004/02/skos/core
# prefix: TimePeriod

@prefix TimePeriod: <http://purl.bdrc.io/ontology/types/TimePeriod/> .
@prefix bdo: <http://purl.bdrc.io/ontology/core/> .
@prefix bdr: <http://purl.bdrc.io/resource/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix adm: <http://purl.bdrc.io/ontology/admin/> .

bdo:ChineseDynasty
  a owl:Class ;
  rdfs:label "Chinese Dynasty"@en ;
  rdfs:label "王朝"@zh-hans ;
  rdfs:subClassOf bdo:ChinesePeriods ;
  adm:translationPriority 2 ;
.
bdo:ChineseEra
  a owl:Class ;
  rdfs:label "Chinese reign" ;
  rdfs:subClassOf bdo:ChinesePeriods ;
  adm:translationPriority 2 ;
.
bdo:ChinesePeriods
  a owl:Class ;
  rdfs:label "Chinese periods"@en ;
  rdfs:subClassOf bdo:TimePeriod ;
  adm:translationPriority 2 ;
.
bdo:QuarterCentury
  a owl:Class ;
  rdfs:label "Quarter Century"@en ;
  rdfs:subClassOf bdo:TimePeriod ;
  adm:translationPriority 2 ;
.
bdo:TimePeriod
  a owl:Class ;
  rdfs:comment "Not currently used. May be incorporated as more info on Chinese dates and so on are included"@en ;
  rdfs:comment "Time periods are specified in terms of year ranges via :notBefore and :notAfter. Years are signed integers using Western \"Common Era\" reckoning."@en ;
  rdfs:label "Time Period"@en ;
  rdfs:seeAlso "bdo:EventOrPeriod"^^xsd:anyURI ;
  adm:translationPriority 2 ;
.
TimePeriod:
  a owl:Ontology ;
  dct:license bda:LicenseCC0 ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
.
