# baseURI: http://www.w3.org/2002/07/owl
# prefix: owl

@prefix : <http://www.w3.org/2002/07/owl#> .
@prefix adm: <http://purl.bdrc.io/ontology/admin/> .
@prefix bdd: <http://purl.bdrc.io/data/> .
@prefix bdo: <http://purl.bdrc.io/ontology/core/> .
@prefix bdr: <http://purl.bdrc.io/resource/> .
@prefix bf: <http://id.loc.gov/ontologies/bibframe/> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix iiif2: <http://iiif.io/api/presentation/2#> .
@prefix iiif3: <http://iiif.io/api/presentation/3#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix tbr: <http://purl.bdrc.io/ontology/toberemoved/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

bdo:FemaleGenderPerson
  a owl:Class ;
  rdfs:subClassOf bdo:Person ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue ""^^bdr:GenderFemale ;
      owl:onProperty bdo:personGender ;
    ] ;
.
bdo:MaleGenderPerson
  a owl:Class ;
  rdfs:subClassOf bdo:Person ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue ""^^bdr:GenderMale ;
      owl:onProperty bdo:personGender ;
    ] ;
.
bdo:hasAunt
  a owl:ObjectProperty ;
  rdfs:label "has aunt"@en ;
  rdfs:range bdo:FemaleGenderPerson ;
.
bdo:hasBrother
  a owl:ObjectProperty ;
  rdfs:label "has brother"@en ;
  rdfs:range bdo:MaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasSibling ;
.
bdo:hasBrotherInLaw
  a owl:ObjectProperty ;
  rdfs:label "has brother in-law"@en ;
  rdfs:range bdo:MaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasSiblingInLaw ;
.
bdo:hasDaughter
  a owl:ObjectProperty ;
  rdfs:label "has daughter"@en ;
  rdfs:range bdo:FemaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasChild ;
.
bdo:hasFather
  a owl:ObjectProperty ;
  rdfs:label "has father"@en ;
  rdfs:range bdo:MaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasParent ;
.
bdo:hasFatherInLaw
  a owl:ObjectProperty ;
  rdfs:label "has father in-law"@en ;
  rdfs:range bdo:MaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasParentInLaw ;
.
bdo:hasGranddaughter
  a owl:ObjectProperty ;
  rdfs:label "has granddaughter"@en ;
  rdfs:range bdo:FemaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasGrandChild ;
.
bdo:hasGrandfather
  a owl:ObjectProperty ;
  rdfs:label "has grandfather"@en ;
  rdfs:range bdo:MaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasGrandParent ;
.
bdo:hasGrandmother
  a owl:ObjectProperty ;
  rdfs:label "has grandmother"@en ;
  rdfs:range bdo:FemaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasGrandParent ;
.
bdo:hasGrandson
  a owl:ObjectProperty ;
  rdfs:label "has grandson"@en ;
  rdfs:range bdo:MaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasGrandChild ;
.
bdo:hasHusband
  a owl:ObjectProperty ;
  rdfs:label "has husband"@en ;
  rdfs:range bdo:MaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasSpouse ;
.
bdo:hasMother
  a owl:ObjectProperty ;
  rdfs:label "has mother"@en ;
  rdfs:range bdo:FemaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasParent ;
.
bdo:hasMotherInLaw
  a owl:ObjectProperty ;
  rdfs:label "has mother in-law"@en ;
  rdfs:range bdo:FemaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasParentInLaw ;
.
bdo:hasNephew
  a owl:ObjectProperty ;
  rdfs:label "has nephew"@en ;
  rdfs:range bdo:MaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasNibling ;
.
bdo:hasNiece
  a owl:ObjectProperty ;
  rdfs:label "has niece"@en ;
  rdfs:range bdo:FemaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasNibling ;
.
bdo:hasSister
  a owl:ObjectProperty ;
  rdfs:label "has sister"@en ;
  rdfs:range bdo:FemaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasSibling ;
.
bdo:hasSisterInLaw
  a owl:ObjectProperty ;
  rdfs:label "has sister in-law"@en ;
  rdfs:range bdo:FemaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasSiblingInLaw ;
.
bdo:hasSon
  a owl:ObjectProperty ;
  rdfs:label "has son"@en ;
  rdfs:range bdo:MaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasChild ;
.
bdo:hasUncle
  a owl:ObjectProperty ;
  rdfs:label "has uncle"@en ;
  rdfs:range bdo:MaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasParentSibling ;
.
bdo:hasWife
  a owl:ObjectProperty ;
  rdfs:label "has wife"@en ;
  rdfs:range bdo:FemaleGenderPerson ;
  rdfs:subPropertyOf bdo:hasSpouse ;
.
bdo:personGender
  a owl:DatatypeProperty ;
.
bdr:GenderFemale
  a rdfs:Datatype ;
.
bdr:GenderMale
  a rdfs:Datatype ;
.
