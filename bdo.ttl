###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdfs:comment """As a convention in the BDRC ontology, use rdfs:comment for summary and contents statements about a resource rather than adding additional properties.

We take the view that the summary or contents statements can be treated in a manner similar to the use of rdfs:label as a way of providing a short description of a resource"""@en ;
             rdfs:label "Comment"@en .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdfs:comment "The rdfs:label is used in the BDRC ontology to provide a label in the :PersonName and :WorkTitle"@en ;
           rdfs:label "label"@en ;
           rdfs:range rdf:PlainLiteral .


###  http://www.w3.org/2000/01/rdf-schema#seeAlso
rdfs:seeAlso rdfs:label "see also"@en ;
             rdfs:range xsd:anyURI ;
             rdfs:domain bdo:Entity .


###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:AnnotationProperty ;
           bdo:inferSubTree "true"^^xsd:boolean ;
           rdfs:label "same as"@en .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty ;
              rdfs:comment "A variant label for entities such as :Place, :Corporation and so on"@en ;
              rdfs:label "variant label"@en ;
              rdfs:subPropertyOf rdfs:label ;
              rdfs:range rdf:PlainLiteral .


###  http://www.w3.org/2004/02/skos/core#hiddenLabel
skos:hiddenLabel rdf:type owl:AnnotationProperty ;
                 rdfs:comment "Used to represent a mispelling of a label that is otherwise intended to be retained in the dataset"@en ;
                 rdfs:label "hidden label"@en ;
                 rdfs:subPropertyOf rdfs:label ;
                 rdfs:range rdf:PlainLiteral .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty ;
               rdfs:comment "The preferred human-readable label for a resource. For a Work this is typically the bibliographic title and will be repeated in a WorkTitle, similarly for Persons"@en ;
               rdfs:label "label"@en ;
               rdfs:subPropertyOf rdfs:label ;
               rdfs:range rdf:PlainLiteral .


#################################################################
#    Datatypes
#################################################################

###  http://purl.bdrc.io/ontology/core/ISBN
bdo:ISBN rdf:type rdfs:Datatype ;
         bdo:dataTypeExpression "^(97(8|9))?\\d{9}(\\d|X)$" ;
         rdfs:comment "The regex is does not take into account opitional blank, ' ', or dash, '-', chars separating the various groups. Also there is no spec for calculating the check digit"@en ;
         rdfs:isDefinedBy <https://www.isbn-international.org/sites/default/files/ISBN%20Manual%202012%20-corr.pdf> ;
         rdfs:label "International Standard Book Number"@en .


###  http://purl.bdrc.io/ontology/core/IntegerOrDate
bdo:IntegerOrDate rdf:type rdfs:Datatype ;
                  rdfs:comment "may be either an integer year or a YYYY-MM-DD date" ;
                  rdfs:label "on date" ;
                  owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                        owl:unionOf ( xsd:date
                                                      xsd:integer
                                                    )
                                      ] .


###  http://purl.bdrc.io/ontology/core/IntegerOrString
bdo:IntegerOrString rdf:type rdfs:Datatype ;
                    owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                          owl:unionOf ( xsd:integer
                                                        xsd:string
                                                      )
                                        ] .


###  http://purl.bdrc.io/ontology/core/LCCN
bdo:LCCN rdf:type rdfs:Datatype ;
         bdo:dataTypeExpression "[[a-z]|[[a-z][a-z]|dd]|[a-z][[a-z][a-z]|dd]|[a-z][a-z]dd]dddddddd" ;
         rdfs:isDefinedBy <http://www.loc.gov/marc/lccn-namespace.html> ;
         rdfs:label "Library of Congress Control Number"@en ;
         owl:equivalentClass xsd:string .


###  http://purl.bdrc.io/ontology/core/LCCallNumber
bdo:LCCallNumber rdf:type rdfs:Datatype ;
                 bdo:dataTypeExpression <https://gist.github.com/kurtraschke/560162> ;
                 rdfs:isDefinedBy <https://www.loc.gov/aba/publications/FreeCSM/freecsm.html> ;
                 rdfs:label "Library of Congress Call Number"@en ;
                 rdfs:seeAlso <https://www.loc.gov/catdir/cpso/lcco/> .


###  http://purl.bdrc.io/ontology/core/LangStringOrNoColophon
bdo:LangStringOrNoColophon rdf:type rdfs:Datatype ;
                           owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                                 owl:unionOf ( bdo:NoColophon
                                                               rdf:PlainLiteral
                                                             )
                                               ] .


###  http://purl.bdrc.io/ontology/core/NoColophon
bdo:NoColophon rdf:type rdfs:Datatype ;
               rdfs:comment "Used to indicated that a Work has no colophon vs a Work for which the colophon has not been recorded in the metadata."@en ;
               rdfs:label "no colophon"@en .


###  http://purl.bdrc.io/ontology/core/SliceLoc
bdo:SliceLoc rdf:type rdfs:Datatype ;
             bdo:dataTypeExpression "[\\d]+:[\\d]+"@en ;
             rdfs:comment """The location within an :Etext of a character within an chunk. The location is a subtype of xsd:string constructed by concatenating:

    chunk number + \":\" + char number within chunk

Both chunk and char number start at one."""@en ;
             rdfs:label "Slice Loc" .


###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://id.loc.gov/ontologies/bibframe/identifiedBy
bf:identifiedBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf bf:objectProperty ;
                owl:inverseOf bf:identifies ;
                rdfs:domain bdo:Work ;
                rdfs:range bf:Identifier .


###  http://id.loc.gov/ontologies/bibframe/identifies
bf:identifies rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf bf:objectProperty .


###  http://id.loc.gov/ontologies/bibframe/objectProperty
bf:objectProperty rdf:type owl:ObjectProperty .


###  http://purl.bdrc.io/ontology/admin/productHasOrg
adm:productHasOrg rdf:type owl:ObjectProperty ;
                  rdfs:domain bdo:ProductOrProductOrg ;
                  rdfs:range adm:ProductOrg ;
                  rdfs:comment "Allows to compose an organization in terms of sub-organizations. Useful for large organizations such as University of California system for which there are many address ranges for each campus such as UC Berkeley and UCLA and so on"@en ,
                               "organization"@en .


###  http://purl.bdrc.io/ontology/core/access
bdo:access rdf:type owl:ObjectProperty ;
           rdfs:domain bdo:TextualEntity ;
           rdfs:range adm:AccessType ;
           rdfs:comment "The type of online access permitted for the subject Work."@en ;
           rdfs:label "text access"@en ;
           rdfs:seeAlso bdo:AccessType .


###  http://purl.bdrc.io/ontology/core/agent
bdo:agent rdf:type owl:ObjectProperty ;
          rdfs:domain bdo:AgentInRole ;
          rdfs:range bdo:Agents ;
          rdfs:comment "identifies the agent participating in an :AgentInRole"@en ;
          rdfs:label "agent"@en .


###  http://purl.bdrc.io/ontology/core/contentProvider
bdo:contentProvider rdf:type owl:ObjectProperty ;
                    rdfs:domain bdo:TextualEntity ;
                    rdfs:range adm:ContentProvider ;
                    rdfs:comment "The organization or person that provided the Etext"@en ;
                    rdfs:label "content provider"@en ;
                    owl:deprecated "deprecated in favor of :hasContentProvider - name change"@en .


###  http://purl.bdrc.io/ontology/core/corporationHasMember
bdo:corporationHasMember rdf:type owl:ObjectProperty ;
                         rdfs:domain bdo:Corporation ;
                         rdfs:range bdo:CorporationMember ;
                         rdfs:label "has member"@en .


###  http://purl.bdrc.io/ontology/core/corporationMember
bdo:corporationMember rdf:type owl:ObjectProperty ;
                      rdfs:domain bdo:CorporationMember ;
                      rdfs:range bdo:Person ;
                      rdfs:label "person"@en .


###  http://purl.bdrc.io/ontology/core/corporationMemberType
bdo:corporationMemberType rdf:type owl:ObjectProperty ;
                          rdfs:domain bdo:CorporationMember ;
                          rdfs:label "type"@en .


###  http://purl.bdrc.io/ontology/core/corporationRegion
bdo:corporationRegion rdf:type owl:ObjectProperty ;
                      rdfs:domain bdo:Corporation ;
                      rdfs:range bdo:Place ;
                      rdfs:comment """
The original corporation.xsd includes several attributes on <region/>: @start, @startCirca, @end, @endCirca that are not used except in one case 
for @start, C71, and a different instance, C13, for @endCirca. The uses are not correct and these attributes have not been included in the current model.

Thus crp:region maps crp:Corporation directly to plc:Place without any additional chaff"""@en ,
                                   "This property records a Place characterizing the region in which the Corporation occured/occurs."@en ;
                      rdfs:label "region"@en .


###  http://purl.bdrc.io/ontology/core/creator
bdo:creator rdf:type owl:ObjectProperty ;
            rdfs:domain bdo:Work ;
            rdfs:range bdo:AgentAsCreator ;
            rdfs:comment ":creator associates a Work with an Agent and a Creator Role that participated in the creation of the Work"@en ;
            rdfs:label "has creator"@en .


###  http://purl.bdrc.io/ontology/core/creatorAmanuensis
bdo:creatorAmanuensis rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf bdo:workCreator ;
                      owl:inverseOf bdo:personAmanuensisOf ;
                      rdfs:comment "the person who writes down a translation in Chinese, in a capacity something like a scribe."@en ;
                      rdfs:label "amanuensis"@en ,
                                 "筆受"@zh-hant .


###  http://purl.bdrc.io/ontology/core/creatorArtist
bdo:creatorArtist rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf bdo:workCreator ;
                  owl:inverseOf bdo:personArtistOf ;
                  rdfs:comment "An artist who provided illustrations and or other non-textual content such as tables, graphs, and the like"@en ;
                  rdfs:label "artist"@en .


###  http://purl.bdrc.io/ontology/core/creatorAttributedAuthor
bdo:creatorAttributedAuthor rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf bdo:workCreator ;
                            owl:inverseOf bdo:personAttributedAuthorOf ;
                            rdfs:comment "A creator identified by a separate source as an author of the Work"@en ;
                            rdfs:label "attributed author"@en .


###  http://purl.bdrc.io/ontology/core/creatorBard
bdo:creatorBard rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf bdo:workCreator ;
                owl:inverseOf bdo:personBardOf ;
                rdfs:comment "for a Work of verse a poet who composed the verse"@en ;
                rdfs:label "bard"@en .


###  http://purl.bdrc.io/ontology/core/creatorCalligrapher
bdo:creatorCalligrapher rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf bdo:workCreator ;
                        owl:inverseOf bdo:personCalligrapherOf ;
                        rdfs:comment "For a handwritten Work an individual that drew the text or carved it on woord blocks"@en ;
                        rdfs:label "calligrapher"@en .


###  http://purl.bdrc.io/ontology/core/creatorCommentator
bdo:creatorCommentator rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf bdo:workCreator ;
                       owl:inverseOf bdo:personCommentatorOf ;
                       rdfs:comment "A person that provided a commentary included in a Work. Used when a Work conventionally comprises root texts and one or more commentaries."@en ;
                       rdfs:label "commentator"@en .


###  http://purl.bdrc.io/ontology/core/creatorCompiler
bdo:creatorCompiler rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf bdo:workCreator ;
                    owl:inverseOf bdo:personCompilerOf ;
                    rdfs:comment """a person who assembles a text out of pre-existing smaller texts or portions, such that the larger text has the nature of a compilation, anthology, chrestomathy etc. Cf., W1KG4337:

Liturgical texts including supplication and recitations for liberating beings from the bardo states. Editor: 
a ti bstan pa'i rgyal mtshan, rdzogs chen 03 (P7404)"""@en ;
                    rdfs:label "compiler"@en .


###  http://purl.bdrc.io/ontology/core/creatorContributingAuthor
bdo:creatorContributingAuthor rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf bdo:workCreator ;
                              owl:inverseOf bdo:personContributingAuthorOf ;
                              rdfs:comment "An individual that author one or more parts of a Work. Used in situations where the Work has a few contributors that are singled out as properties of the Work or when only partial authorship is known, cf. W23280"@en ;
                              rdfs:label "contributing author"@en .


###  http://purl.bdrc.io/ontology/core/creatorEditor
bdo:creatorEditor rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf bdo:workCreator ;
                  adm:deprecatedComment "deprecated in favor of creatorCompiler"@en ;
                  adm:replaceWithObjectProperty bdo:creatorCompiler ;
                  rdfs:label "editor"@en ;
                  owl:deprecated "true"^^xsd:boolean .


###  http://purl.bdrc.io/ontology/core/creatorForger
bdo:creatorForger rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf bdo:workCreator ;
                  rdfs:comment "a person who produces a text, but then knowingly presents it as the end result of some other process of composition--usually, as a translation from an Indic language, and sometimes under the pretense that it was \"translated\" by a person other than the forger (i.e. with a false ascription attached)."@en ;
                  rdfs:label "forger"@en ,
                             "偽作"@zh-hant .


###  http://purl.bdrc.io/ontology/core/creatorGeneralAuthor
bdo:creatorGeneralAuthor rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf bdo:workCreator ;
                         adm:deprecatedComment "deprecated in favor of :creatorMainAuthor"@en ;
                         adm:replaceWithObjectProperty bdo:creatorMainAuthor ;
                         rdfs:label "general author"@en ;
                         owl:deprecated "true"^^xsd:boolean .


###  http://purl.bdrc.io/ontology/core/creatorHeadTranslator
bdo:creatorHeadTranslator rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf bdo:workCreator ;
                          owl:inverseOf bdo:personHeadTranslatorOf ;
                          rdfs:comment "The person identified explicitly or treated by the tradition implicitly as the head of the translation group or workshop that produced a translation. Such persons may be explicitly identified by the tradition as such, but often our knowledge that this was in fact a person's role is implicit or inferred from other knowledge about the composition of the translation group."@en ;
                          rdfs:label "head translator"@en ,
                                     "譯主"@zh-hant .


###  http://purl.bdrc.io/ontology/core/creatorIndicScholar
bdo:creatorIndicScholar rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf bdo:workCreator ;
                        owl:inverseOf bdo:personIndicScholarOf ;
                        rdfs:comment "in the case of Chinese texts, the persons who function in the translation group as experts in the Indic traditions in question can sometimes be identified primarily by other function labels, such as \"head translator\"; we use this creator type primarily as an equivalent for persons whom the Chinese tradition states \"handled the Indic text\", Chn. (shou) zhi Fan (wen) [手]執梵[文], (shou) zhi Hu (ben) [手]執胡[本] etc.; this role can also overlap with that of \"reciter\""@en ;
                        rdfs:label "Indic Scholar"@en .


###  http://purl.bdrc.io/ontology/core/creatorMainAuthor
bdo:creatorMainAuthor rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf bdo:workCreator ;
                      owl:inverseOf bdo:personMainAuthorOf ;
                      rdfs:comment "the actual author of a text, sometimes against assertions in the tradition that the text was authored by someone else or produced by processes other than direct authorship, such as translation; in numerous cases, the exact force of this role characterisation can depend upon the nature or genre of the text in question; e.g. to claim that a Chinese person is the \"author\" of a sutra implies that the text was composed in China, but such texts must necessarily almost universally be presented by the tradition as if they were translated from Indic originals (since sutras are buddhavacana, and the Buddha was generally understood to have been active in India); but the implications might be quite different if we identify someone as the \"author\" of a commentary"@en ;
                      rdfs:label "main author"@en .


###  http://purl.bdrc.io/ontology/core/creatorOralTranslator
bdo:creatorOralTranslator rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf bdo:workCreator ;
                          owl:inverseOf bdo:personOralTranslatorOf ;
                          rdfs:comment "a person who actually renders a text from a foreign language into Chinese, in a manner that the tradition clearly states was spoken rather than written; Chn. chuanyu 傳語, kouxuan 口宣[...言], chuanyi 傳譯, duyu 度語 etc."@en ;
                          rdfs:label "oral translator"@en .


###  http://purl.bdrc.io/ontology/core/creatorPandita
bdo:creatorPandita rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf bdo:workCreator ;
                   adm:deprecatedComment "deprecated in favor of :creatorIndicScholar"@en ;
                   adm:replaceWithObjectProperty bdo:creatorIndicScholar ;
                   rdfs:label "pandita"@en ;
                   owl:deprecated "true"^^xsd:boolean .


###  http://purl.bdrc.io/ontology/core/creatorRecipientOfResponseToQuery
bdo:creatorRecipientOfResponseToQuery rdf:type owl:ObjectProperty ;
                                      rdfs:subPropertyOf bdo:workCreator ;
                                      owl:inverseOf bdo:personRecipientOfResponseToQueryOf ;
                                      rdfs:comment """An individual to whom answers in a Work are addressed. Cf., W22146:

Study of the history of Buddhism in Tibet. Wirtten by the 16th century author, Yagde Dulzin Khyenrab Gyatso. Written in response to polemical questions by Karmapa 08 Mikyo Dorje (1507-1554) raised against the Nyingma order."""@en ;
                                      rdfs:label "recipient of response to query"@en .


###  http://purl.bdrc.io/ontology/core/creatorReciter
bdo:creatorReciter rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf bdo:workCreator ;
                   owl:inverseOf bdo:personReciterOf ;
                   rdfs:comment "a person who recites an original text (usually Indic) in an original (usually Indic) language during the process of translation; Chn. koushou 口授, tongxuan 同宣 etc., though this function can also sometimes overlap with \"IndicScholar\" (see below)"@en ;
                   rdfs:label "reciter"@en .


###  http://purl.bdrc.io/ontology/core/creatorRequester
bdo:creatorRequester rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf bdo:workCreator ;
                     adm:deprecatedComment "deprecated - not used"@en ;
                     rdfs:comment "An individual who requested the author to write the Work"@en ;
                     rdfs:label "requester"@en ;
                     owl:deprecated "true"^^xsd:boolean .


###  http://purl.bdrc.io/ontology/core/creatorReviser
bdo:creatorReviser rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf bdo:workCreator ;
                   owl:inverseOf bdo:personReviserOf ;
                   rdfs:label """An individual who is known to have revised a previoius work or translation. Cf., W3CN4970:

The root text of Pramanavartika by Dharmakirti translated by Khache Kelden Gyalpo and Ngog Lotsava Loden Sherap and revised by Shakyashri Bhadra and Sapan Kunga Gyatsen."""@en ,
                              "reviser"@en .


###  http://purl.bdrc.io/ontology/core/creatorReviserOfTranslation
bdo:creatorReviserOfTranslation rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf bdo:workCreator ;
                                adm:replaceWithObjectProperty bdo:creatorReviser ;
                                rdfs:comment """An individual who is known to have revised a previoius translation. Cf., W3CN4970:

The root text of Pramanavartika by Dharmakirti translated by Khache Kelden Gyalpo and Ngog Lotsava Loden Sherap and revised by Shakyashri Bhadra and Sapan Kunga Gyatsen."""@en ;
                                rdfs:label "reviser of translation"@en ;
                                owl:deprecated "true"^^xsd:boolean .


###  http://purl.bdrc.io/ontology/core/creatorScribe
bdo:creatorScribe rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf bdo:workCreator ;
                  owl:inverseOf bdo:personScribeOf ;
                  rdfs:comment "An individual that wrote the text typically during recitation by the author. Cf., W8145"@en ;
                  rdfs:label "scribe"@en .


###  http://purl.bdrc.io/ontology/core/creatorTerton
bdo:creatorTerton rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf bdo:workCreator ;
                  owl:inverseOf bdo:personTertonOf ;
                  rdfs:comment """An individual who revealed the Work as treasure texts. Cf., W25816:

Practice text for the secret sadhana of Ganapati according to revelations by Diri Chodrak"""@en ;
                  rdfs:label "tertön"@en .


###  http://purl.bdrc.io/ontology/core/creatorTranslator
bdo:creatorTranslator rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf bdo:workCreator ;
                      owl:inverseOf bdo:personTranslatorOf ;
                      rdfs:comment """A translator of the Work. Cf., W8LS25412:

The glorious blissful garland ,the root sadhana of the dakini by Jigme Lingpa, translated by Tulku Thondup"""@en ;
                      rdfs:label "translator"@en .


###  http://purl.bdrc.io/ontology/core/creatorUnspecified
bdo:creatorUnspecified rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf bdo:workCreator ;
                       rdfs:comment "a person for whom our source or evidence tells us only that they were involved in the translation process, without allowing us to further specify the specific role they played in that process. Use bdr:UNKNOWN_Person when no specific person is known"@en ;
                       rdfs:label "unspecified"@en .


###  http://purl.bdrc.io/ontology/core/eTextHasChunk
bdo:eTextHasChunk rdf:type owl:ObjectProperty ;
                  rdfs:domain bdo:Etext ;
                  rdfs:range bdo:EtextChunk ;
                  rdfs:label "has chunk"@en .


###  http://purl.bdrc.io/ontology/core/eTextHasPage
bdo:eTextHasPage rdf:type owl:ObjectProperty ;
                 rdfs:domain bdo:Etext ;
                 rdfs:range bdo:EtextPage ;
                 rdfs:comment """Refers to a page of an eText corresponding to a physical/image page asset.

Used to provide stand-off markup for the pagination of eText in correspondence with physical/image assets when known such as in OCR or certain cases of manual input where such information has been preserved during input."""@en ;
                 rdfs:label "page"@en .


###  http://purl.bdrc.io/ontology/core/eTextInItem
bdo:eTextInItem rdf:type owl:ObjectProperty ;
                rdfs:domain bdo:Etext ;
                rdfs:range bdo:ItemEtext ;
                rdfs:comment "The reference to the :ItemEtext indicates the item that contains the volume that this Etext is contained in"@en ;
                rdfs:label "in item"@en ;
                rdfs:seeAlso bdo:Etext .


###  http://purl.bdrc.io/ontology/core/eTextResource
bdo:eTextResource rdf:type owl:ObjectProperty ;
                  rdfs:domain bdo:EtextRef ;
                  rdfs:range bdo:Etext ;
                  rdfs:comment "Refers to an :Etext that is contained in a particular :VolumeEtextAsset"@en ;
                  rdfs:label "eText resource"@en .


###  http://purl.bdrc.io/ontology/core/eventPeriod
bdo:eventPeriod rdf:type owl:ObjectProperty ;
                rdfs:domain bdo:Event ;
                rdfs:range bdo:TimePeriod ;
                rdfs:comment "associates an event with a time period"@en ;
                rdfs:label "period"@en .


###  http://purl.bdrc.io/ontology/core/eventWhere
bdo:eventWhere rdf:type owl:ObjectProperty ;
               rdfs:domain bdo:Event ;
               rdfs:range bdo:Place ;
               rdfs:label "place"@en .


###  http://purl.bdrc.io/ontology/core/eventWho
bdo:eventWho rdf:type owl:ObjectProperty ;
             rdfs:domain bdo:Event ;
             rdfs:range bdo:PersonOrCorporation ;
             rdfs:label "who"@en .


###  http://purl.bdrc.io/ontology/core/hasAunt
bdo:hasAunt rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf bdo:hasParentSibling ;
            rdfs:label "has aunt"@en .


###  http://purl.bdrc.io/ontology/core/hasBrother
bdo:hasBrother rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf bdo:hasSibling ;
               rdf:type owl:SymmetricProperty ;
               rdfs:label "has brother"@en .


###  http://purl.bdrc.io/ontology/core/hasBrotherInLaw
bdo:hasBrotherInLaw rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf bdo:hasSiblingInLaw ;
                    rdfs:label "has brother in-law"@en .


###  http://purl.bdrc.io/ontology/core/hasChild
bdo:hasChild rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf bdo:kinWith ;
             owl:inverseOf bdo:hasParent ;
             rdfs:label "has child"@en .


###  http://purl.bdrc.io/ontology/core/hasContentProvider
bdo:hasContentProvider rdf:type owl:ObjectProperty ;
                       rdfs:domain bdo:TextualEntity ;
                       rdfs:range bdo:AnyOrganization ;
                       rdfs:comment "The organization (or person) that provided the subject :TextualEntity. In this context, providing can mean originating the enscribing, carving or woodblocks and printing, or creating a collection of texts from other providers. For example, the sde dge par khang can be recorded as the :Organization that provided the sde dge bka' 'gyur."@en ;
                       rdfs:label "has content provider"@en .


###  http://purl.bdrc.io/ontology/core/hasCousin
