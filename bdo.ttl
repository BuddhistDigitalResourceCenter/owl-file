###  http://purl.bdrc.io/ontology/core/workPartIndex
bdo:workPartIndex rdf:type owl:DatatypeProperty ;
                  rdfs:domain bdo:Work ;
                  rdfs:range xsd:integer ;
                  rdfs:comment """The part index is used to provide a simple way to order the parts of a parent :Work. The direct children parts of a :Work are indexed from 1 to n. The direct children of each of these is similarly indexed from 1 to n.

Schematically:

W1
   :workHasPart W1_0001, W1_0005 ;

W1_0001
    :workHasPart W1_0002, W1_0003, W1_0004 ;
    :workPartOf W1 ;
    :workIndex 1 ;

W1_0002
    :workPartOf W1_0001 ;
    :workIndex 1 ;

W1_0003
    :workPartOf W1_0001 ;
    :workIndex 2 ;

W1_0004
    :workPartOf W1_0001 ;
    :workIndex 3 ;

W1_0005
    :workHasPart W1_0006, W1_0007 ;
    :workPartOf W1 ;
    :workIndex 2 ;

W1_0006
    :workPartOf W1_0005 ;
    :workIndex 1 ;

W1_0007
    :workPartOf W1_0005 ;
    :workIndex 2 ;"""@en ;
                  rdfs:label "part index"@en .


